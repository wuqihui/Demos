@{
    ViewBag.Title = " 演示 - Web Uploader";
    Layout = null;
}

<!DOCTYPE html>
<html lang="zh-CN">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        演示 - Web Uploader
    </title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Scripts/webUploader/webuploader.css" rel="stylesheet" />
    <link href="~/Content/Demo.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <script src="~/Scripts/webUploader/webuploader.js"></script>
    <script src="~/Scripts/CustomerJs/demo.js"></script>

</head>
<body>
    <table border="8">
        <tr>
            <th>Month</th>
        </tr>
        <tr>
            <td style="padding: 10px 10px 10px 10px">@Html.Partial("PartialWebuploader", new { WebuploaderIndex = "1" })</td>
          
        </tr>
        <tr>
            <td style="padding: 10px 10px 10px 10px">@Html.Partial("PartialWebuploader", new { WebuploaderIndex = "2" })</td>
        </tr>
    </table>
    <script type="text/javascript" language="javascript">
        $(document).ready(function () {
            InitWebloader(1);
            InitWebloader(2);
        });
    </script>

</body>
</html>
